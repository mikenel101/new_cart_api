{
    "name": "Mikes Cart API",
    "description": "Mikes Cart API",
    "keywords": ["mike", "cart"],
    "license": "None",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.3.*",
        "vlucas/phpdotenv": "~2.2",
        "doctrine/dbal": "^2.5",
        "rutorika/sortable": "^3.2",
        "dimsav/laravel-translatable": "^6.0",
        "kalnoy/nestedset": "^4.2",
        "alsofronie/eloquent-uuid": "dev-master",
        "guzzlehttp/guzzle": "~6.2.2",
        "prettus/l5-repository": "^2.6",
        "dingo/api": "1.0.x@dev",
        "illuminate/redis": "5.3.*",
        "predis/predis": "~1.1",
        "ext-bcmath": "*",
        "bschmitt/laravel-amqp": "1.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9",
        "satooshi/php-coveralls": "dev-master",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "MikesLumenApi\\": "vendor_local/mikes-lumen-api/src/",
            "MikesLumenRepository\\": "vendor_local/mikes-lumen-repository/src/",
            "MikesLumenBase\\": "vendor_local/mikes-lumen-base/src/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
